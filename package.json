{
  "name": "simple-react-popover",
  "version": "1.0.5",
  "description": "A Popover component built with react",
  "main": "dist/Popover.js",
  "scripts": {
    "test": "mocha --require ./test/testSetup.js \"./**/*.spec.js\"",
    "prepare": "webpack",
    "babel": "babel src --ignore *.spec.js --ignore *.story.js --out-dir ./dist --presets=@babel/env",
    "start": "start-storybook",
    "build": "build-storybook -o public",
    "deploy": "now --token=$NOW_TOKEN --public && now --token=$NOW_TOKEN alias",
    "build-prod": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jampies/React_Popover.git"
  },
  "keywords": [
    "popover",
    "react",
    "overlay"
  ],
  "author": "Murray le Roux",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jampies/React_Popover/issues"
  },
  "homepage": "https://github.com/jampies/React_Popover#readme",
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-knobs": "^5.2.0",
    "@storybook/react": "^5.2.0",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "jsdom": "^15.1.1",
    "metro-react-native-babel-preset": "^0.56.0",
    "mocha": "^6.2.0",
    "mock-css-modules": "^1.0.0",
    "now": "^16.2.0",
    "testdouble": "^3.12.4",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8"
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "classnames": "^2.2.6",
    "css-loader": "^3.2.0",
    "node-sass": "^4.12.0",
    "postcss-loader": "^3.0.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "sass-loader": "^8.0.0",
